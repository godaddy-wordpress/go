{
  "name": "go",
  "version": "1.8.1",
  "description": "Go is an innovative, Gutenberg-first WordPress theme, hyper-focused on empowering makers to build beautifully rich websites with WordPress.",
  "homepage": "https://github.com/godaddy-wordpress/go",
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy-wordpress/go",
    "base": "godaddy-wordpress/go"
  },
  "author": {
    "name": "GoDaddy",
    "url": "https://www.godaddy.com",
    "role": "Developer"
  },
  "license": "GPL-2.0-or-later",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start --progress",
    "lint:css": "wp-scripts lint-style -f verbose",
    "lint:js": "wp-scripts lint-js",
    "lint:php": "wp-env run composer run-script lint",
    "makepot": "composer run-script makepot && npm run pot2json",
    "pot2json": ".dev/bin/pot2json languages/go.pot languages/go.json",
    "json2po": "cd languages && find . -name '*.json' ! -name 'go.json' -execdir /bin/bash -c 'FROM=\"$0\" && TO=\"go-`basename $0 .json`.po\" && echo \"$FROM > $TO\" && ../.dev/bin/json2po go.json $FROM $TO && msgmerge --previous $TO go.pot > $TO-msgmerge && mv $TO-msgmerge $TO && if [[ \"$OSTYPE\" == \"darwin\"* ]]; then sed -i \"\" -e \"/^#, fuzzy$/d\" $TO; else sed -i -e \"/^#, fuzzy$/d\" $TO; fi;' '{}' \\;",
    "po2mo": "cd languages && find . -name '*.json' ! -name 'go.json' -execdir /bin/bash -c 'FROM=\"go-`basename $0 .json`.po\" && TO=\"`basename $0 .json`.mo\" && echo \"$FROM > $TO\" && msgfmt $FROM -o $TO' '{}'  \\;",
    "packages-update": "wp-scripts packages-update --dist-tag=wp-6.2",
    "postpackages-update": "npm run build",
    "prelint:php": "wp-env start && wp-env run composer \"install --no-interaction\"",
    "pretest:unit:php": "wp-env start --xdebug=develop,coverage",
    "test:unit:php": "wp-env run tests-wordpress /var/www/html/wp-content/themes/$(basename $(pwd))/vendor/bin/phpunit -c /var/www/html/wp-content/themes/$(basename $(pwd))/phpunit.xml.dist --verbose --coverage-html /var/www/html/wp-content/themes/$(basename $(pwd))/phpunit/coverage",
    "test:a11y": "node .dev/tests/accessibility/compliance/pa11y.js",
    "test:js": "wp-scripts test-unit-js --config .dev/tests/jest/jest.config.js",
    "test:js:coverage": "npm test:js --coverage",
    "test:js:update": "npm test:js -u",
    "wp-env": "wp-env"
  },
  "locales": {
    "ar": "العربية",
    "da_DK": "Dansk",
    "de_DE": "Deutsch",
    "el": "Ελληνικά",
    "es_ES": "Español",
    "fi": "Suomi",
    "fr_FR": "Français",
    "hi_IN": "हिन्दी",
    "id_ID": "Bahasa Indonesia",
    "it_IT": "Italiano",
    "ja": "日本語",
    "ko_KR": "한국어",
    "mr": "मराठी",
    "nb_NO": "Norsk bokmål",
    "nl_NL": "Nederlands",
    "pl_PL": "Polski",
    "pt_BR": "Português do Brasil",
    "pt_PT": "Português",
    "ru_RU": "Русский",
    "sv_SE": "Svenska",
    "th": "ไทย",
    "tr_TR": "Türkçe",
    "uk": "Українська",
    "vi": "Tiếng Việt",
    "zh_CN": "简体中文",
    "zh_TW": "繁體中文"
  },
  "testing": {
    "urls": {
      "local": "http://localhost:8888/",
      "staging": "",
      "production": ""
    },
    "accessibility": {
      "compliance": "WCAG2A"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@jest/core": "^29.5.0",
    "@godaddy-wordpress/eslint-config": "^0.6.1",
    "@godaddy-wordpress/stylelint-config": "^0.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@wordpress/env": "^5.11.1",
    "@wordpress/scripts": "^25.3.4",
    "jest-fetch-mock": "^3.0.3",
    "pa11y": "^6.2.3",
    "prop-types": "^15.8.1",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@wordpress/base-styles": "^4.17.2",
    "@wordpress/components": "^23.9.0",
    "@wordpress/dom": "^3.32.0",
    "@wordpress/dom-ready": "^3.26.1",
    "@wordpress/element": "^5.3.2"
  }
}

    "@wordpress/element": "^4.0.3",
    "@wordpress/jest-preset-default": "^8.0.1",
    "@wordpress/env": "^4.2.2",
    "@wordpress/scripts": "^20.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
